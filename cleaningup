pipeline {
	stages {
		stage('Build') {
			steps {
				sh 'pwd'
				sh 'echo "****"'
				sh 'ls'
			}
		}
	}
	post {
		always {
			echo 'One way or another, I have finished'
			deleteDir()
		}
		success {
			echo 'I succeeeded!'
		}
		failure {
			echo 'I failed :('
		}
		changed {
			echo 'Things were different before...'
		}
	}
}
