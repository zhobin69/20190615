pipeline {
	agent any
	environment {
		name = "zhobin"
	}
	stages {
		stage('Build') {
			steps {
				echo "Hello, this is Build stage"
				sh 'echo "**********************"'
			}
		}
		stage('Deploy') {
			when {
				environment name: "name" , value: "zhobin"
			}
			steps {
				echo "THIS STAGE IS DEPLOY"
			}
		}	
	}
}
